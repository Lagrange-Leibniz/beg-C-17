#include <iostream>
#include <iomanip>
#include <cmath>
#include <vector>

int main() {
    size_t n {};
    std::cout << "请输入向量元素的数量：";
    std::cin >> n;

    std::vector<double> values(n);

    for (size_t i {}; i < n; ++i)
        values[i] = 1.0 / ((i + 1) * (i + 1));

    double sum {};
    for (const auto& value : values)
        sum += value;

    std::cout << "结果是 " << std::setprecision(15) << std::sqrt(6.0 * sum) << std::endl;

    // 不需要手动释放内存，std::vector 会自动管理内存
}
