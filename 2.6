#include <iostream>

int main() {
    long a, b;

    std::cout << "Enter a positive integer: ";
    std::cin >> a;
    std::cout << "Enter another different positive integer: ";
    std::cin >> b;

    if (a < b) {
        std::swap(a, b);
    }

    const long larger = a;
    const long smaller = b;

    std::cout << "The larger integer is " << larger << ".\n"
              << "The smaller integer is " << smaller << '.' << std::endl;
}
